{
	"schemaVersion": 1,
	"id": "lifeseries",
	"version": "${version}+${minecraftVersion}",
	"name": "Life Series",
	"description": "A one-to-one recreation of Grian's Life Series in fabric.",
	"authors": [
		"Mat0u5"
	],
	"contributors": [
		"RowwYourBoat (Rowan)"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Mat0u5/LifeSeries"
	},
	"license": "MIT",
	"icon": "assets/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.mat0u5.lifeseries.Main"
		],
		"client": [
			"net.mat0u5.lifeseries.MainClient"
		],
		"voicechat": [
          "net.mat0u5.lifeseries.compatibilities.voicechat.VoicechatMain"
		]
	},
	"mixins": [
		"lifeseries.mixins.json",
		{
			"config": "lifeseries.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "${targetVersion}",
		"java": ">=${javaVersion}",
		"fabric-api": "${fabricApiVersion}"
	},
	"breaks": {
		"pastlife": "*"
	},
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/class_3222": ["net/mat0u5/lifeseries/utils/interfaces/IServerPlayer"],
			"net/minecraft/server/level/ServerPlayer": ["net/mat0u5/lifeseries/utils/interfaces/IServerPlayer"]
		}
	}
}